<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#ffffff" />
        </shape>
    </item>
    <item
        android:left="0.5dp"
        android:right="0.5dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape>
            <solid android:color="#53bcea" />
            <padding
                android:left="0.5dp"
                android:right="0.5dp"
                android:top="1dp"
                android:bottom="1dp"
                />
        </shape>
    </item>
</layer-list>
